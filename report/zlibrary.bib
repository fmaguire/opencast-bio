	pages = {905--918},
	file = {IEEE Xplore Abstract Record:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/Z3WH49JA/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/S6IWH34F/Dong et al. - 2014 - Clustering on Multi-Layer Graphs via Subspace Anal.pdf:application/pdf}
