	pages = {2309--20},
	file = {Liu et al. - 1998 - A model neuron with activity-dependent conductances regulated by multiple calcium sensors:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/CRM64GWS/Liu et al. - 1998 - A model neuron with activity-dependent conductances regulated by multiple calcium sensors.pdf:application/pdf}
