	pages = {3327--3338},
	file = {ScienceDirect Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/G9UZJVBC/Bell and Sejnowski - 1997 - The “independent components” of natural scenes are.pdf:application/pdf;ScienceDirect Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/N2WWNZD7/S0042698997001211.html:text/html}
}

