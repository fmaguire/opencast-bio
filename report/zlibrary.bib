	keywords = {Coincidence detection, Computational model, Human Genetics, Mushroom body, Neurology, Neurosciences, Non-elemental, Olfactory pathways, Theory of Computation},
	pages = {197--212},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/J9FNSWP6/Wessnitzer et al. - 2012 - A model of non-elemental olfactory learning in Dro.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/93XCEFJ5/10.html:text/html}
