	pmid = {19689962},
	pages = {2962--2968},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/2QFB65KH/Kashima et al. - 2009 - Simultaneous inference of biological networks of m.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/B7M8ANBS/2962.html:text/html}
