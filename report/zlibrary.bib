@article{hengen_firing_2013-1,
	title = {Firing rate homeostasis in visual cortex of freely behaving rodents.},
	volume = {80},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/24139038},
	doi = {10.1016/j.neuron.2013.08.038},
	abstract = {It has been postulated that homeostatic mechanisms maintain stable circuit function by keeping neuronal firing within a set point range, but such firing rate homeostasis has never been demonstrated in vivo. Here we use chronic multielectrode recordings to monitor firing rates in visual cortex of freely behaving rats during chronic monocular visual deprivation ({MD}). Firing rates in V1 were suppressed over the first 2 day of {MD} but then rebounded to baseline over the next 2-3 days despite continued {MD}. This drop and rebound in firing was accompanied by bidirectional changes in {mEPSC} amplitude measured ex vivo. The rebound in firing was independent of sleep-wake state but was cell type specific, as putative {FS} and regular spiking neurons responded to {MD} with different time courses. These data establish that homeostatic mechanisms within the intact {CNS} act to stabilize neuronal firing rates in the face of sustained sensory perturbations.},
	number = {2},
	journal = {Neuron},
	author = {Hengen, Keith B and Lambo, Mary E and Van Hooser, Stephen D and Katz, Donald B and Turrigiano, Gina G},
	month = oct,
	year = {2013},
	pages = {335--42},
	file = {Hengen et al. - 2013 - Firing rate homeostasis in visual cortex of freely behaving rodents:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/K293ZRXW/Hengen et al. - 2013 - Firing rate homeostasis in visual cortex of freely behaving rodents.pdf:application/pdf}
}

