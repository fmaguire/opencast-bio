	pages = {1688--1701},
	file = {IEEE Xplore Abstract Record:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/BNBPIQ6U/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/KPR88ISP/Buccigrossi and Simoncelli - 1999 - Image compression via joint statistical characteri.pdf:application/pdf}
