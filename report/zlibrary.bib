@article{sussillo_generating_2009,
	title = {Generating coherent patterns of activity from chaotic neural networks.},
	volume = {63},
	issn = {1097-4199},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2756108&tool=pmcentrez&rendertype=abstract},
