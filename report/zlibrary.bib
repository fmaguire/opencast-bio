	keywords = {Animals, {CA}3 Region, Hippocampal, {CA}3 Region, Hippocampal: cytology, {CA}3 Region, Hippocampal: physiology, Cells, Cultured, Hippocampus, Hippocampus: cytology, Hippocampus: physiology, Homeostasis, Homeostasis: physiology, Mossy Fibers, Hippocampal, Mossy Fibers, Hippocampal: physiology, Neuronal Plasticity, Neuronal Plasticity: physiology, Neurons, Neurons: physiology, Rats, Synapses, Synapses: physiology},
