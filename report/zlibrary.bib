	keywords = {Animal structures, Biological information theory, Biology computing, Circuits, Constraint theory, feature-analyzing function, Genetics, information theory, Intelligent networks, multilayered networks, network connections, neural nets, Neural networks, Neuroscience, Optimization, Pattern Recognition, perceptual network, self-adjusting systems, self organization, System testing, visual system},
