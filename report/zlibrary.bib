	author = {Qi, Yanjun and Balem, Fernanda and Faloutsos, Christos and Klein-Seetharaman, Judith and Bar-Joseph, Ziv},
	month = jul,
	year = {2008},
	pmid = {18586722},
	pages = {i250--i268},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/JZTA328P/Qi et al. - 2008 - Protein complex identification by supervised graph.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/Q77BUQZK/i250.html:text/html}
}

