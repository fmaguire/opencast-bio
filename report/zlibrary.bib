	pages = {1426--1435},
	file = {IEEE Xplore Abstract Record:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/7MNG4S3X/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/KJEJQPHZ/Adhikari et al. - 2012 - Memristor Bridge Synapse-Based Neural Network and .pdf:application/pdf}
