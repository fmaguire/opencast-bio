	month = oct,
	year = {2012},
	keywords = {Imaging, Model organisms, Neuroscience},
	pages = {1818--1829},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/B4TA2664/Chen et al. - 2012 - LOTOS-based two-photon calcium imaging of dendriti.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/6UHHB347/nprot.2012.106.html:text/html}
