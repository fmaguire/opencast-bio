	keywords = {Algorithm Analysis and Problem Complexity, Logics and Meanings of Programs, Mathematical Logic and Formal Languages, Programming Languages, Compilers, Interpreters, Programming Techniques, Software Engineering},
	pages = {26--42},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/C99F7U59/Chen et al. - 2011 - Time-Bounded Verification of CTMCs against Real-Ti.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/DKTTD9HU/978-3-642-24310-3_4.html:text/html}
