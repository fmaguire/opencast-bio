	month = aug,
	year = {2010},
	keywords = {Affinity isolation, Analytical Chemistry, Biochemistry, general, brain, Environmental Monitoring/Analysis, Food Science, Laboratory Medicine, Measurement Science and Instrumentation, Protein interactome, Synapse},
	pages = {3195--3202},
	file = {Full Text PDF:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/KRP5463M/Li et al. - 2010 - Interaction proteomics of synapse protein complexe.pdf:application/pdf;Snapshot:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/5AUU4FTH/s00216-010-3658-z.html:text/html}
