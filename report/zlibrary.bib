	journal = {The European journal of neuroscience},
	author = {Karmarkar, Uma R and Buonomano, Dean V},
	month = mar,
	year = {2006},
	keywords = {2-Amino-5-phosphonovalerate, 2-Amino-5-phosphonovalerate: pharmacology, 6-Cyano-7-nitroquinoxaline-2,3-dione, 6-Cyano-7-nitroquinoxaline-2,3-dione: pharmacology, Animals, Electrophysiology, Excitatory Amino Acid Antagonists, Excitatory Amino Acid Antagonists: pharmacology, Excitatory Postsynaptic Potentials, Excitatory Postsynaptic Potentials: physiology, Hippocampus, Hippocampus: drug effects, Hippocampus: physiology, Homeostasis, Homeostasis: drug effects, Homeostasis: physiology, Motor Activity, Motor Activity: physiology, Nerve Net, Nerve Net: drug effects, Nerve Net: physiology, Neuronal Plasticity, Neuronal Plasticity: drug effects, Neuronal Plasticity: physiology, Organ Culture Techniques, Patch-Clamp Techniques, Rats, Rats, Sprague-Dawley, Tetrodotoxin, Tetrodotoxin: pharmacology},
	pages = {1575--84},
	file = {Karmarkar, Buonomano - 2006 - Different forms of homeostatic plasticity are engaged with distinct temporal profiles:/home/gavin/.mozilla/firefox/i2j16z7y.default/zotero/storage/H68BU3CP/Karmarkar, Buonomano - 2006 - Different forms of homeostatic plasticity are engaged with distinct temporal profiles.pdf:application/pdf}
